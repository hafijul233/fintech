{
    "name": "hafijul233/fintech",
    "type": "project",
    "description": "The personal financial and expense counting system",
    "keywords": [
        "framework",
        "laravel",
        "expense",
        "income",
        "debit",
        "credit"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alexwenzel/nova-dependency-container": "^1.7",
        "badinansoft/nova-language-switch": "^1.0",
        "coroowicaksono/chart-js-integration": "^0.4.0",
        "eminiarts/nova-tabs": "^2.2",
        "formfeed-uk/nova-breadcrumbs": "^1.1",
        "greg0x46/masked-field": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.19",
        "laravel/nova": "^4.18",
        "laravel/nova-log-viewer": "^0.1.4",
        "laravel/sanctum": "^3.0",
        "lonnyx/nova-apex-chart": "^1.3",
        "oneduo/nova-file-manager": "^0.7.2",
        "spatie/nova-backup-tool": "^5.0",
        "visanduma/nova-back-navigation": "^1.0",
        "wdelfuego/nova-calendar": "^1.6",
        "wdelfuego/nova4-customizable-footer": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "laravel/tinker": "^2.7",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan storage:link --ansi"

        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=nova-assets --ansi --force"

        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
